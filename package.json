{
  "name": "warclaude",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/bebeal/warclaude"
  },
  "bugs": {
    "url": "https://github.com/bebeal/warclaude/issues"
  },
  "scripts": {
    "build": "bun clean && NODE_ENV=production bun build:client && bun build:server",
    "build:client": "tsc && vite build",
    "build:server": "tsc -p tsconfig.node.json && vite build --ssr src/entry-server.tsx",
    "build:cdk": "CDK_OUTDIR=cdk.out tsx cdk/cdk.ts",
    "clean": "rimraf dist cdk.out",
    "dev": "nodemon --watch server --exec tsx server",
    "deploy": "dotenv -- cdk deploy --all --require-approval never && ./scripts/sync-dns.sh",
    "format": "prettier --write src server.ts",
    "lint": "bun eslint src server.ts --report-unused-disable-directives --max-warnings 0",
    "postbuild": "chmod +x ./scripts/postbuild.sh && ./scripts/postbuild.sh && bun build:cdk",
    "prepare": "husky",
    "serve": "node dist/server/server.js",
    "test": "vitest run",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@bebeal/rehype-code-terminal": "^0.3.2",
    "@bebeal/rehype-color-chips": "^1.0.1",
    "@bebeal/ripple-effect": "^2.1.0",
    "@carbon/icons-react": "^11.70.0",
    "@mdx-js/react": "^3.1.1",
    "@mdx-js/rollup": "^3.1.1",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-router": "^1.157.17",
    "@tanstack/react-start": "^1.157.17",
    "compression": "^1.8.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-tweet": "^3.2.2",
    "rehype-katex": "^7.0.1",
    "rehype-starry-night": "^2.2.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-mdx-frontmatter": "^5.2.0",
    "serve-static": "^2.2.0",
    "serverless-http": "^4.0.0",
    "source-map-support": "^0.5.21",
    "vite": "^7.3.1"
  },
  "devDependencies": {
    "@bebeal/console-prefix-plugin": "^0.1.7",
    "@eslint/js": "^9.39.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@storybook/react-vite": "^10.2.12",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/eslint-plugin-router": "^1.133.19",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@tanstack/react-router-devtools": "^1.157.17",
    "@tanstack/router-plugin": "^1.157.17",
    "@types/compression": "^1.8.1",
    "@types/eslint": "^9.6.1",
    "@types/express": "^5.0.5",
    "@types/node": "^25.1.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/serve-static": "^2.2.0",
    "@types/source-map-support": "^0.5.10",
    "@vitejs/plugin-react": "^5.1.2",
    "@wooorm/starry-night": "^3.8.0",
    "aws-cdk": "^2.1104.0",
    "aws-cdk-lib": "^2.225.0",
    "constructs": "^10.4.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "9.39.1",
    "eslint-plugin-mdx": "^3.6.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "next-themes": "^0.4.6",
    "nodemon": "^3.1.11",
    "prettier": "3.8.1",
    "rimraf": "^6.1.0",
    "storybook": "^10.2.12",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite-plugin-node-polyfills": "^0.25.0",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^4.0.18"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "4.57.1",
    "@rollup/rollup-linux-arm64-gnu": "4.57.1",
    "@rollup/rollup-linux-x64-gnu": "4.57.1",
    "@rollup/rollup-win32-x64-msvc": "4.57.1"
  },
  "engines": {
    "node": ">=20",
    "bun": ">=1"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "arm64",
    "x64",
    "ia32"
  ],
  "resolutions": {
    "glob": "^10.5.0"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  }
}
